{
  "languageserver": {
    "dart": {
      "command": "dart",
      "args": [
        "/opt/dart-sdk/bin/snapshots/analysis_server.dart.snapshot",
        "--lsp",
        "--client-id",
        "vim",
        "--client-version",
        "coc.nvim"
      ],
      "filetypes": [
        "dart"
      ],
      "trace.server": "verbose"
    }
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "dart"
  ],
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "wxss.validate": false,
  "stylelintplus.autoFixOnFormat": true
}
