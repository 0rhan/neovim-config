{
  "languageserver": {
      "dart": {
            "command": "dart",
            "args": [
                    "/opt/dart-sdk/bin/snapshots/analysis_server.dart.snapshot",
                    "--lsp",
                    "--client-id",
                    "vim",
                    "--client-version",
                    "coc.nvim"
                  ],
            "filetypes": ["dart"],
            "trace.server": "verbose"
          }
    },
  "coc.preferences.formatOnSaveFiletypes": [
    "dart"
  ],
"explorer.focus": true,
"explorer.toggle": true,
"explorer.previewAction.onHover": "content",
"explorer.activeMode":true,
"explorer.openAction.strategy": "tab",
"explorer.icon.enableNerdfont": true,
"explorer.buffer.tabOnly": true,
"explorer.file.autoReveal": true,
"explorer.file.showHiddenFiles": true,
"explorer.file.column.git.showIgnored": true,
"explorer.file.tabCD": true,
"emmet.includeLanguages": {
    "javascript": "javascriptreact"
  }
}
